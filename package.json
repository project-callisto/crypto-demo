        "ajv": "^6.2.1",
        "babel-core": "^6.26.0",
        "faker": "^4.1.0",
        "jasmine": "^3.1.0",
        "jasmine-core": "^2.99.1",
        "jasmine-expect-count": "^1.1.0",
        "jasmine-spec-reporter": "^4.2.1",
        "jasmine-ts": "^0.2.1",
        "jsdom": "^11.6.2",
        "karma": "^2.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-jasmine": "^1.1.1",
        "karma-spec-reporter": "0.0.32",
        "protractor": "^5.3.0",
        "tslint": "^5.9.1",
        "webdriver-manager": "^12.0.6"
    }
